<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>NoiseSupressor</defName>
    <label>noise supressor</label>
    <description>A sound processor connected to a powerful speaker, capable of analizing the surrounding sounds and emitting an inverse waveform, thus cancelling harmful noises.</description>
    <graphicData>
      <texPath>Things/Building/Misc/FirefoamPopper</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shadowData>
        <volume>(0.25, 0.35, 0.25)</volume>
        <offset>(0,0,-0.1)</offset>
      </shadowData>
      <damageData>
        <rect>(0.2,0.2,0.6,0.6)</rect>
      </damageData>
    </graphicData>
    <comps>
      <li Class="KeepItQuiet.CompProperties_Soother">
        <power>12</power>
        <maxLevel>12</maxLevel>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>50</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <tickerType>Normal</tickerType>
    <altitudeLayer>Building</altitudeLayer>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <WorkToBuild>1500</WorkToBuild>
      <Mass>10</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>-3</Beauty>
    </statBases>
    <fillPercent>0.30</fillPercent>
    <costList>
      <Steel>40</Steel>
      <Gold>4</Gold>
      <ComponentIndustrial>2</ComponentIndustrial>
    </costList>
    <specialDisplayRadius>9.6</specialDisplayRadius>
    <pathCost>25</pathCost>
    <designationCategory>Misc</designationCategory>
    <rotatable>false</rotatable>
    <!--<researchPrerequisites>
      <li>XXXXXXXX</li>
    </researchPrerequisites>-->
    <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
  </ThingDef>
</Defs>